{
  "applicationId": "com.zet.gui",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "./www/",
  "port": 0,
  "url": "./www/index.html",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "enableExtensions": false,
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "filesystem.*",
    "window.*",
    "computer.*",
    "storage.*",
    "debug.*"
  ],
  "modes": {
    "window": {
      "title": "ZetGui",
      "width": 900,
      "height": 600,
      "minWidth": 700,
      "minHeight": 500,
      "center": true,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "./resources/icon.png",
      "enableInspector": true,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": true,
      "processArgs": "--disable-gpu --disable-software-rasterizer --disable-background-timer-throttling --disable-backgrounding-occluded-windows --disable-renderer-backgrounding --disable-features=TranslateUI --disable-ipc-flooding-protection --no-sandbox --disable-web-security --disable-compositing"
    }
  },
  "cli": {
    "binaryName": "zetgui",
    "resourcesPath": "./www/",
    "extensionsPath": "./extensions/",
    "frontendLibrary": {
      "patchFile": "./www/index.html",
      "devUrl": "http://localhost:3002",
      "projectPath": "./react-src/",
      "initCommand": "npm install",
      "devCommand": "npm start",
      "buildCommand": "npm run build"
    }
  }
}